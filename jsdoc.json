{
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "source": {
        "include": ["README.md"]
    },
    "docdash": {
        "search": true,
        "scripts": [
            "scripts/seatchart.js",
            "styles/seatchart.css",
            "scripts/docs.js",
            "styles/docs.css"
        ],
        "menu":{
            "Demo":{
                "href":"https://seatchart.js.org/demo.html",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            },
            "Github":{
                "href":"https://github.com/omarmahili/seatchart.js",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            }
        }
    }
}
