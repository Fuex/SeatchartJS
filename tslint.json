{
    "extends": [
        "tslint:recommended",
        "tslint-microsoft-contrib/recommended"
    ],
    "rules": {
        "indent": [true, "spaces", 4],
        "no-angle-bracket-type-assertion": false,
        "no-var-keyword": true,
        "quotemark": [true, "single"],
        "interface-name": false,
        "import-name": false,
        "export-name": false,
        "function-name": false,
        "prefer-array-literal": false
    }
}
